    url('../fonts/layout.woff?1b74gi') format('woff'),
    url('../fonts/layout.svg?1b74gi#layout') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
